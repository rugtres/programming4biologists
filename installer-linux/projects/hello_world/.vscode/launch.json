{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${fileDirname}/bin/${workspaceFolderBasename}",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${fileDirname}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
        "logging": {
          "trace": false,
          "traceResponse": false,
          "engineLogging": false,
          "moduleLoad": false
        },
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          },
          {
            "description": "Set Disassembly Flavor to Intel",
            "text": "-gdb-set disassembly-flavor intel",
            "ignoreFailures": true
          }
        ]
      }
    ]
}